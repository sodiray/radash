    "./string": {
      "import": {
        "types": "./dist/string.d.mts",
        "default": "./dist/string.mjs"
      },
      "types": "./dist/string.d.ts",
      "default": "./dist/string.js"
    },
    "./typed": {
      "import": {
        "types": "./dist/typed.d.mts",
        "default": "./dist/typed.mjs"
      },
      "types": "./dist/typed.d.ts",
      "default": "./dist/typed.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/aleclarson/radashi"
  },
  "author": "Alec Larson",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "tsup --clean --watch --sourcemap",
    "build": "tsup --clean",
    "lint": "tsc -p . --noEmit",
    "test": "jest --coverage",
    "docs:install": "pnpm add next@12.3.4 -D",
    "docs:build": "chiller build --ci"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/jest": "^28.1.1",
    "chai": "^4.3.6",
    "chiller": "1.0.0-rc.30",
    "fast-glob": "^3.3.2",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.3",
    "ts-jest": "^28.0.8",
    "tsup": "^8.1.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14.18.0"
  }
}
